package com.fastbiding.model;

public class CompletedProjects {
      
	
	private int rtid;
	private int vid;
	private int pjid;
	private int comment;
	
	public CompletedProjects() {
		// TODO Auto-generated constructor stub
	}

	public CompletedProjects(int rtid, int vid, int pjid, int comment) {
		super();
		this.rtid = rtid;
		this.vid = vid;
		this.pjid = pjid;
		this.comment = comment;
	}

	public int getRtid() {
		return rtid;
	}

	public void setRtid(int rtid) {
		this.rtid = rtid;
	}

	public int getVid() {
		return vid;
	}

	public void setVid(int vid) {
		this.vid = vid;
	}

	public int getPjid() {
		return pjid;
	}

	public void setPjid(int pjid) {
		this.pjid = pjid;
	}

	public int getComment() {
		return comment;
	}

	public void setComment(int comment) {
		this.comment = comment;
	}

	@Override
	public String toString() {
		return "CompletedProjects [rtid=" + rtid + ", vid=" + vid + ", pjid=" + pjid + ", comment=" + comment + "]";
	}
	
	
}
